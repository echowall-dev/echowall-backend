{
  "name": "Bubble",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true
    },
    "orientation": {
      "type": "string",
      "default": "SW"
    },
    "positionX": {
      "type": "number",
      "default": 0.5
    },
    "positionY": {
      "type": "number",
      "default": 0.5
    },
    "playCount": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "audio": {
      "type": "hasOne",
      "model": "Audio",
      "property": "audio"
    },
    "creator": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "creatorId"
    }
  },
  "acls": [],
  "methods": {}
}
