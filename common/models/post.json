{
  "name": "Post",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "uuid": {
      "type": "string",
      "defaultFn": "uuidv4"
    },
    "caption": {
      "type": "string",
      "default": null
    },
    "privacy": {
      "type": "string",
      "default": "friend"
    },
    "status": {
      "type": "string",
      "default": "active"
    }
  },
  "validations": [],
  "relations": {
    "creator": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "creatorId"
    },
    "collaborators": {
      "type": "hasMany",
      "model": "Account",
      "foreignKey": "postId",
      "through": "Collaboration",
      "keyThrough": "collaboratorId"
    },
    "likers": {
      "type": "hasMany",
      "model": "Account",
      "through": "Like",
      "polymorphic": "likeable"
    },
    "image": {
      "type": "embedsOne",
      "model": "Image",
      "property": "image"
    },
    "bubbles": {
      "type": "embedsMany",
      "model": "Bubble",
      "property": "bubbles"
    }
  },
  "acls": [],
  "methods": {}
}
